#############################
# ALVOS (PESSOAS / ORCRIM)
#############################

# Alvo principal / suspeito
v5:targetP_001 a v5:TargetPerson ;
  v5:personFullName "João da Silva" ;
  v5:cpf "111.222.333-44" .

# Vítima
v5:targetP_002 a v5:TargetPerson ;
  v5:personFullName "Maria Oliveira" ;
  v5:cpf "555.666.777-88" .

# Testemunha
v5:targetP_003 a v5:TargetPerson ;
  v5:personFullName "Carlos Souza" ;
  v5:cpf "999.000.111-22" .

# Organização criminosa
v5:org_CO001 a v5:CriminalOrganization ;
  rdfs:label "Orcrim Hidra Vermelha" .

# Relações caso ↔ alvos
v5:case_C001 v5:hasPrimaryTarget v5:targetP_001 ;
                v5:hasSuspectTarget v5:targetP_001 ;
                v5:hasVictimTarget v5:targetP_002 ;
                v5:hasWitnessTarget v5:targetP_003 .

# Alvo membro de orcrim
v5:targetP_001 v5:memberOfOrganization v5:org_CO001 .

#############################
# TELEFONES E DISPOSITIVOS
#############################

# Telefones
v5:phone_joao a v5:PhoneNumber ;
  v5:phoneNumberValue "+5521999990001" .

v5:phone_maria a v5:PhoneNumber ;
  v5:phoneNumberValue "+5521988880002" .

# Dispositivos
v5:device_joao a v5:Device ;
  v5:deviceIMEI "356789012345678" ;
  v5:deviceModel "Samsung Galaxy S21" .

v5:device_maria a v5:Device ;
  v5:deviceIMEI "357654321098765" ;
  v5:deviceModel "iPhone 13" .

# Ligações alvo ↔ telefone ↔ dispositivo
v5:targetP_001 v5:hasPhoneNumber v5:phone_joao ;
                  v5:usesDevice     v5:device_joao .

v5:targetP_002 v5:hasPhoneNumber v5:phone_maria ;
                  v5:usesDevice     v5:device_maria .

#############################
# UFDR / EXTRAÇÃO
#############################

# Extração UFDR do dispositivo de João
v5:ufdr_001 a v5:UFDRExtraction ;
  rdfs:label "UFDR João - 2025-10-01" ;
  v5:wasExtractedFromDevice v5:device_joao .

# Evidência digital principal ligada à UFDR (ex: diretório UFDR no disco)
v5:evidence_UFDR_001 a v5:DigitalEvidence ;
  rdfs:label "Dump UFDR João" ;
  v5:belongsToExtraction v5:ufdr_001 .

# Caso 1 tem essa evidência UFDR
v5:case_C001 v5:hasEvidence v5:evidence_UFDR_001 .

#############################
# WHATSAPP – CHATS E MENSAGENS
#############################

# Chat entre João e Maria
v5:waChat_CH001 a v5:WhatsAppChat ;
  rdfs:label "Chat João ↔ Maria" ;
  v5:hasParticipant v5:targetP_001 , v5:targetP_002 .

# Mensagens no chat
v5:waMsg_M001 a v5:WhatsAppMessage ;
  v5:messageBody "Transferi o valor combinado." ;
  v5:messageTimestamp "2025-10-01T10:30:00"^^xsd:dateTime ;
  v5:sentBy  v5:targetP_001 ;
  v5:sentTo  v5:targetP_002 ;
  v5:belongsToChat v5:waChat_CH001 .

v5:waMsg_M002 a v5:WhatsAppMessage ;
  v5:messageBody "Ok, recebi aqui. Amanhã entrego o material." ;
  v5:messageTimestamp "2025-10-01T10:32:00"^^xsd:dateTime ;
  v5:sentBy  v5:targetP_002 ;
  v5:sentTo  v5:targetP_001 ;
  v5:belongsToChat v5:waChat_CH001 .

# A extração UFDR “contém” esse chat e suas mensagens
v5:ufdr_001 v5:hasChat v5:waChat_CH001 .
v5:waChat_CH001 v5:hasMessage v5:waMsg_M001 , v5:waMsg_M002 .

#############################
# EVIDÊNCIA – IMAGEM FACIAL / BIOMETRIA
#############################

# Evidência de imagem facial (foto anexada ao caso)
v5:evidence_FACE_001 a v5:EvidenceItem ;
  rdfs:label "Foto de João em frente ao veículo" .

# Arquivo de imagem
v5:file_FACEIMG_001 a v5:FaceImage ;
  rdfs:label "face_joao_carro.jpg" ;
  v5:filePath "/cases/C001/images/face_joao_carro.jpg" ;
  v5:fileMimeType "image/jpeg" .

# Ligação evidência -> imagem -> alvo
v5:evidence_FACE_001 v5:hasFaceImage v5:file_FACEIMG_001 .
v5:file_FACEIMG_001 v5:depictsPerson v5:targetP_001 .

# Caso 1 tem essa evidência facial
v5:case_C001 v5:hasEvidence v5:evidence_FACE_001 .

# Template biométrico gerado a partir da imagem
v5:template_FACE_001 a v5:FaceBiometricTemplate ;
  v5:biometricTemplateType "FACE_EMBEDDING_V1" ;
  v5:biometricTemplateData "ENCODED_VECTOR_ABC123" .

# Ligação (opcional) evidência -> template
v5:evidence_FACE_001 v5:hasFaceTemplate v5:template_FACE_001 .

#############################
# DETRAN – VEÍCULO / PLACA / REGISTRO / MOVIMENTAÇÃO
#############################

# Veículo de João
v5:veh_001 a v5:Vehicle ;
  v5:vehicleBrand "Toyota" ;
  v5:vehicleModel "Corolla" ;
  v5:vehicleYear "2018"^^xsd:gYear ;
  v5:chassisNumber "9BWZZZ377VT004251" .

# Placa do veículo
v5:plate_ABC1234 a v5:LicensePlate ;
  v5:licensePlateNumber "ABC1D23" .

# Registro do veículo
v5:vehReg_001 a v5:VehicleRegistration ;
  v5:renavam "12345678901" .

# Amarrações
v5:veh_001 v5:hasLicensePlate       v5:plate_ABC1234 ;
              v5:hasVehicleRegistration v5:vehReg_001 .

v5:vehReg_001 v5:isRegistrationOf v5:veh_001 .

# João é proprietário do veículo
v5:targetP_001 v5:ownsVehicle v5:veh_001 .

# Movimentação do veículo (passagem em câmera)
v5:loc_L001 a v5:Location ;
  rdfs:label "Av. Brasil, altura 5000 - Rio de Janeiro" .

v5:vehMov_001 a v5:VehicleMovement ;
  v5:movementTimestamp "2025-10-01T09:50:00"^^xsd:dateTime ;
  v5:isMovementOf       v5:veh_001 ;
  v5:movementAtLocation v5:loc_L001 .

# Evidência digital representando o registro DETRAN
v5:evidence_VEH_REG_001 a v5:EvidenceItem ;
  rdfs:label "Certidão de registro do veículo Corolla" .

# Evidência digital representando a captura da movimentação
v5:evidence_VEH_MOV_001 a v5:EvidenceItem ;
  rdfs:label "Frame de câmera mostrando Corolla na Av. Brasil" .

# Ligação evidência -> veículo (via registro/movimento)
# (essas duas linhas permitem às property chains inferir involvesVehicle / involvesTarget)
v5:evidence_VEH_REG_001 v5:hasFile v5:vehReg_001 .
# v5:evidence_VEH_MOV_001 v5:hasFile v5:vehMov_001 .
# Arquivo de imagem/vídeo da movimentação
v5:file_VEH_MOV_001 a v5:ImageFile ;
  rdfs:label "frame_corolla_av_brasil.jpg" ;
  v5:filePath "/cases/C001/images/frame_corolla_av_brasil.jpg" ;
  v5:fileMimeType "image/jpeg" .

# Evidência aponta para o arquivo (não diretamente para a Activity)
v5:evidence_VEH_MOV_001 v5:hasFile v5:file_VEH_MOV_001 .

# (Opcional) Dizemos que este arquivo diz respeito à movimentação específica
# (se quiser, podemos depois criar uma propriedade própria, tipo 'representsMovement')


# Caso 1 tem essas evidências de veículo
v5:case_C001 v5:hasEvidence v5:evidence_VEH_REG_001 ,
                             v5:evidence_VEH_MOV_001 .

#############################
# CASO 2 – SÓ VEÍCULO E TESTEMUNHA
#############################

# Caso 2 só com veículo e testemunha, sem WhatsApp
v5:case_C002 v5:hasWitnessTarget v5:targetP_003 ;
                v5:hasEvidence      v5:evidence_VEH_MOV_001 .


