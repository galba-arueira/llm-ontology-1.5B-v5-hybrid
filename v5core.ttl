@prefix rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:    <http://www.w3.org/2002/07/owl#> .
@prefix xsd:    <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf:   <http://xmlns.com/foaf/0.1/> .
@prefix prov:   <http://www.w3.org/ns/prov#> .
@prefix vcard:  <http://www.w3.org/2006/vcard/ns#> .
@prefix geo:    <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix schema: <https://schema.org/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix v5:  <https://ontology.v5.com/v5core#> .

@base <https://ontology.v5.com/v5core#> .

<https://ontology.v5.com/v5core> a owl:Ontology ;
  rdfs:label "Ontologia v5 Core"@pt ,
             "v5 Core Ontology"@en ;
  owl:versionInfo "0.1.0" ;
  dcterms:creator "Galba Arueira" ;
  dcterms:created "2025-11-20"^^xsd:date ;
  rdfs:comment "Ontologia núcleo para o v5, cobrindo Casos, Alvos, Telefones, UFDR, WhatsApp, Detran e Arquivos."@pt ;
  owl:imports <http://xmlns.com/foaf/0.1/> ,
              <http://www.w3.org/ns/prov-o#> ,
              <http://www.w3.org/2006/vcard/ns#> ,
              <http://www.w3.org/2003/01/geo/wgs84_pos#> ,
              <https://schema.org/> .

########################
# CLASSES PRINCIPAIS  #
########################

### Núcleo investigativo

v5:Case a owl:Class ;
  rdfs:subClassOf prov:Entity ;
  rdfs:label "Caso"@pt , "Case"@en , "processo"@pt , "investigação"@pt , "procedimento"@pt ;
  rdfs:comment "Investigação ou processo legal contendo alvos, evidências e relatórios."@pt ;
  v5:hasImportantProperty "caseNumber" .

v5:Incident a owl:Class ;
  rdfs:subClassOf prov:Entity ;
  rdfs:label "Ocorrência"@pt , "Incident"@en ;
  rdfs:comment "Ocorrência policial, denúncia, boletim ou fato correlato a um caso."@pt .

v5:Target a owl:Class ;
  rdfs:subClassOf foaf:Agent ;
  rdfs:label "Alvo"@pt , "Target"@en , "investigado"@pt , "suspeito"@pt ;
  rdfs:comment "Pessoa física ou jurídica foco de investigação."@pt .

v5:TargetPerson a owl:Class ;
  rdfs:subClassOf v5:Target , foaf:Person ;
  rdfs:label "Alvo Pessoa"@pt , "Target Person"@en , "dono"@pt , "proprietário"@pt , "pessoa"@pt , "pessoas"@pt , "indivíduo"@pt , "investigado"@pt ;
  rdfs:comment "Pessoa física identificada como alvo em um ou mais casos."@pt ;
  v5:hasImportantProperty "cpf" .
  

v5:TargetOrganization a owl:Class ;
  rdfs:subClassOf v5:Target , schema:Organization ;
  rdfs:label "Alvo Organização"@pt , "Target Organization"@en , "empresa"@pt , "organização"@pt , "entidade"@pt ;
  rdfs:comment "Pessoa jurídica ou organização alvo de investigação."@pt .

v5:CriminalOrganization a owl:Class ;
  rdfs:subClassOf v5:TargetOrganization ;
  rdfs:label "Organização Criminosa"@pt , "Criminal Organization"@en ;
  rdfs:comment "Grupo formal ou informal reconhecido como organização criminosa."@pt .

v5:Unit a owl:Class ;
  rdfs:subClassOf foaf:Organization ;
  rdfs:label "Unidade"@pt , "Unit"@en ;
  rdfs:comment "Unidade policial ou órgão responsável pelo caso ou ocorrência."@pt .

v5:User a owl:Class ;
  rdfs:subClassOf foaf:Person ;
  rdfs:label "Usuário v5"@pt , "v5 User"@en ;
  rdfs:comment "Usuário do sistema v5, agente humano autenticado."@pt .

### Localização

v5:Location a owl:Class ;
  rdfs:subClassOf geo:SpatialThing ;
  rdfs:label "Localização"@pt , "Location"@en ;
  rdfs:comment "Localização geográfica relevante para casos, ocorrências ou movimentos de veículos."@pt .

### Telefones e dispositivos

v5:PhoneNumber a owl:Class ;
  rdfs:subClassOf vcard:Telephone ;
  rdfs:label "Número de Telefone"@pt , "Phone Number"@en , "celular"@pt , "telefone"@pt ;
  rdfs:comment "Número de telefone associado a um alvo, dispositivo ou conta de comunicação."@pt .

v5:Device a owl:Class ;
  rdfs:subClassOf prov:Entity ;
  rdfs:label "Dispositivo"@pt , "Device"@en , "aparelho"@pt , "equipamento"@pt ;
  rdfs:comment "Dispositivo físico, como smartphone, tablet ou outro equipamento digital."@pt .

### UFDR e comunicações

v5:DeviceExtraction a owl:Class ;
  rdfs:subClassOf prov:Activity ;
  rdfs:label "Extração de Dispositivo"@pt , "Device Extraction"@en ;
  rdfs:comment "Procedimento de extração de dados de um dispositivo, como relatórios UFDR."@pt .

v5:UFDRExtraction a owl:Class ;
  rdfs:subClassOf v5:DeviceExtraction ;
  rdfs:label "Extração UFDR"@pt , "UFDR Extraction"@en ;
  rdfs:comment "Extração de dados realizada por ferramenta Cellebrite UFDR."@pt .

v5:WhatsAppChat a owl:Class ;
  rdfs:subClassOf prov:Entity ;
  rdfs:label "Chat WhatsApp"@pt , "WhatsApp Chat"@en ;
  rdfs:comment "Conversa ou grupo de WhatsApp extraído de um dispositivo ou fonte externa."@pt .

v5:WhatsAppMessage a owl:Class ;
  rdfs:subClassOf v5:Communication ;
  rdfs:label "Mensagem WhatsApp"@pt , "WhatsApp Message"@en ;
  rdfs:comment "Mensagem individual de WhatsApp, associada a um chat e participantes."@pt .

v5:WhatsAppGroup a owl:Class ;
  rdfs:subClassOf v5:WhatsAppChat ;
  rdfs:label "Grupo WhatsApp"@pt , "WhatsApp Group"@en ;
  rdfs:comment "Grupo de WhatsApp com múltiplos participantes."@pt .

v5:WhatsAppContact a owl:Class ;
  rdfs:subClassOf v5:Contact ;
  rdfs:label "Contato WhatsApp"@pt , "WhatsApp Contact"@en ;
  rdfs:comment "Contato de WhatsApp identificado em um dispositivo ou conta."@pt .

### Comunicação genérica

v5:Communication a owl:Class ;
  rdfs:subClassOf prov:Entity ;
  rdfs:label "Comunicação"@pt , "Communication"@en ;
  rdfs:comment "Abstração para comunicações (mensagens, ligações, etc.)."@pt .

v5:Contact a owl:Class ;
  rdfs:subClassOf foaf:Agent ;
  rdfs:label "Contato"@pt , "Contact"@en ;
  rdfs:comment "Contato genérico (agenda, rede social, etc.)."@pt .

### Evidências e arquivos

v5:EvidenceItem a owl:Class ;
  rdfs:subClassOf prov:Entity ;
  rdfs:label "Item de Evidência"@pt , "Evidence Item"@en ;
  rdfs:comment "Item de evidência associado a um caso (digital ou documental)."@pt .

v5:DigitalEvidence a owl:Class ;
  rdfs:subClassOf v5:EvidenceItem ;
  rdfs:label "Evidência Digital"@pt , "Digital Evidence"@en ;
  rdfs:comment "Evidência em formato digital, como arquivos, logs, mídias extraídas."@pt .

v5:File a owl:Class ;
  rdfs:subClassOf v5:DigitalEvidence ;
  rdfs:label "Arquivo"@pt , "File"@en ;
  rdfs:comment "Arquivo digital armazenado ou referenciado pelo v5."@pt .

v5:ImageFile a owl:Class ;
  rdfs:subClassOf v5:File ;
  rdfs:label "Arquivo de Imagem"@pt , "Image File"@en ;
  rdfs:comment "Arquivo de imagem digital (foto, captura de tela, etc.)."@pt .

v5:VideoFile a owl:Class ;
  rdfs:subClassOf v5:File ;
  rdfs:label "Arquivo de Vídeo"@pt , "Video File"@en ;
  rdfs:comment "Arquivo de vídeo digital."@pt .

v5:AudioFile a owl:Class ;
  rdfs:subClassOf v5:File ;
  rdfs:label "Arquivo de Áudio"@pt , "Audio File"@en ;
  rdfs:comment "Arquivo de áudio digital."@pt .

v5:DocumentFile a owl:Class ;
  rdfs:subClassOf v5:File ;
  rdfs:label "Documento Digital"@pt , "Document File"@en ;
  rdfs:comment "Documento digital (PDF, DOC, planilha, etc.)."@pt .

v5:OCRTextSegment a owl:Class ;
  rdfs:subClassOf v5:DigitalEvidence ;
  rdfs:label "Segmento OCR"@pt , "OCR Text Segment"@en ;
  rdfs:comment "Trecho de texto resultante de OCR aplicado a uma imagem ou documento."@pt .

v5:FaceImage a owl:Class ;
  rdfs:subClassOf v5:ImageFile ;
  rdfs:label "Imagem Facial"@pt , "Face Image"@en ;
  rdfs:comment "Imagem contendo rosto(s) utilizada para reconhecimento ou análise facial."@pt .

v5:FaceBiometricTemplate a owl:Class ;
  rdfs:subClassOf v5:DigitalEvidence ;
  rdfs:label "Template Biométrico Facial"@pt , "Face Biometric Template"@en ;
  rdfs:comment "Representação matemática de características faciais para biometria."@pt .

### Veículos / Detran

v5:Vehicle a owl:Class ;
  rdfs:subClassOf prov:Entity ;
  rdfs:label "Veículo"@pt , "Vehicle"@en , "carro"@pt , "carros"@pt , "moto"@pt , "motos"@pt , "caminhão"@pt , "veículo"@pt , "veículos"@pt , "automóvel"@pt , "automóveis"@pt ;
  rdfs:comment "Veículo automotor registrado em órgão de trânsito."@pt .

v5:LicensePlate a owl:Class ;
  rdfs:subClassOf prov:Entity ;
  rdfs:label "Placa Veicular"@pt , "Placa"@pt , "License Plate"@en ;
  v5:compositeExamplePattern "Quem é o dono do veículo de placa <VALOR>|Quem é o dono do carro de placa <VALOR>|Buscar dono do carro com placa <VALOR>"@pt , "Who is the owner of the vehicle with plate <VALOR>|Who is the owner of the car with plate <VALOR>|Find the car owner with plate <VALOR>"@en ;
  v5:hasImportantProperty "licensePlateNumber" .

v5:VehicleRegistration a owl:Class ;
  rdfs:subClassOf prov:Entity ;
  rdfs:label "Registro de Veículo"@pt , "Vehicle Registration"@en ;
  rdfs:comment "Registro administrativo do veículo (ex.: RENAVAM, cadastro no DETRAN)."@pt .

v5:VehicleMovement a owl:Class ;
  rdfs:subClassOf prov:Activity ;
  rdfs:label "Movimentação de Veículo"@pt , "Vehicle Movement"@en ;
  rdfs:comment "Evento de movimentação ou uso do veículo (passagem em câmera, blitz, abordagem etc.)."@pt .

########################
# DISJOINTS            #
########################

v5:Case owl:disjointWith v5:Target ,
                         v5:Device ,
                         v5:Vehicle ,
                         v5:EvidenceItem .

v5:Vehicle owl:disjointWith foaf:Person ,
                               v5:Case ,
                               v5:Device .

v5:FaceBiometricTemplate owl:disjointWith v5:ImageFile .

########################
# OBJECT PROPERTIES    #
########################

### Casos e alvos

v5:hasTarget a owl:ObjectProperty ;
  rdfs:domain v5:Case ;
  rdfs:range v5:Target ;
  owl:inverseOf v5:isTargetOf ;
  rdfs:label "tem alvo"@pt , "has target"@en .

v5:isTargetOf a owl:ObjectProperty ;
  rdfs:domain v5:Target ;
  rdfs:range v5:Case ;
  rdfs:label "é alvo de"@pt , "is target of"@en .

v5:handledByUnit a owl:ObjectProperty ;
  rdfs:domain v5:Case ;
  rdfs:range v5:Unit ;
  rdfs:label "é conduzido por unidade"@pt , "handled by unit"@en .

v5:hasIncident a owl:ObjectProperty ;
  rdfs:domain v5:Case ;
  rdfs:range v5:Incident ;
  rdfs:label "tem ocorrência"@pt , "has incident"@en .

v5:relatesToCase a owl:ObjectProperty ;
  rdfs:domain v5:Incident ;
  rdfs:range v5:Case ;
  rdfs:label "relaciona-se ao caso"@pt , "relates to case"@en .

v5:memberOfOrganization a owl:ObjectProperty ;
  rdfs:domain v5:TargetPerson ;
  rdfs:range v5:CriminalOrganization ;
  rdfs:label "membro de organização criminosa"@pt ,
             "member of criminal organization"@en .

### Telefones, dispositivos e comunicação

v5:hasPhoneNumber a owl:ObjectProperty ;
  rdfs:domain v5:Target ;
  rdfs:range v5:PhoneNumber ;
  rdfs:label "tem número de telefone"@pt ,
             "has phone number"@en .

v5:usesDevice a owl:ObjectProperty ;
  rdfs:domain v5:Target ;
  rdfs:range v5:Device ;
  rdfs:label "usa dispositivo"@pt ,
             "uses device"@en .

v5:wasExtractedFromDevice a owl:ObjectProperty ;
  rdfs:domain v5:DeviceExtraction ;
  rdfs:range v5:Device ;
  rdfs:label "foi extraída de dispositivo"@pt ,
             "was extracted from device"@en .

v5:producedDigitalEvidence a owl:ObjectProperty ;
  rdfs:domain v5:DeviceExtraction ;
  rdfs:range v5:DigitalEvidence ;
  rdfs:label "produziu evidência digital"@pt ,
             "produced digital evidence"@en .

v5:belongsToExtraction a owl:ObjectProperty ;
  rdfs:domain v5:DigitalEvidence ;
  rdfs:range v5:DeviceExtraction ;
  rdfs:label "pertence à extração"@pt ,
             "belongs to extraction"@en .

v5:hasChat a owl:ObjectProperty ;
  rdfs:domain v5:DeviceExtraction ;
  rdfs:range v5:WhatsAppChat ;
  rdfs:label "possui chat WhatsApp"@pt ,
             "has WhatsApp chat"@en .

v5:hasMessage a owl:ObjectProperty ;
  rdfs:domain v5:WhatsAppChat ;
  rdfs:range v5:WhatsAppMessage ;
  rdfs:label "possui mensagem"@pt ,
             "has message"@en .

v5:sentBy a owl:ObjectProperty ;
  rdfs:domain v5:WhatsAppMessage ;
  rdfs:range v5:Target ;
  rdfs:label "enviada por"@pt , "sent by"@en .

v5:sentTo a owl:ObjectProperty ;
  rdfs:domain v5:WhatsAppMessage ;
  rdfs:range v5:Target ;
  rdfs:label "enviada para"@pt , "sent to"@en .

v5:hasParticipant a owl:ObjectProperty ;
  rdfs:domain v5:WhatsAppChat ;
  rdfs:range v5:Target ;
  rdfs:label "tem participante"@pt , "has participant"@en .

### Evidências e arquivos

v5:hasEvidence a owl:ObjectProperty ;
  rdfs:domain v5:Case ;
  rdfs:range v5:EvidenceItem ;
  rdfs:label "tem evidência"@pt , "has evidence"@en .

v5:hasFile a owl:ObjectProperty ;
  rdfs:domain v5:EvidenceItem ;
  rdfs:range v5:File ;
  rdfs:label "tem arquivo"@pt , "has file"@en .

v5:hasOCRText a owl:ObjectProperty ;
  rdfs:domain v5:EvidenceItem ;
  rdfs:range v5:OCRTextSegment ;
  rdfs:label "tem texto OCR"@pt , "has OCR text"@en .

v5:hasFaceImage a owl:ObjectProperty ;
  rdfs:domain v5:EvidenceItem ;
  rdfs:range v5:FaceImage ;
  rdfs:label "tem imagem facial"@pt , "has face image"@en .

v5:hasFaceTemplate a owl:ObjectProperty ;
  rdfs:domain v5:EvidenceItem ;
  rdfs:range v5:FaceBiometricTemplate ;
  rdfs:label "tem template facial"@pt , "has face template"@en .

v5:depictsPerson a owl:ObjectProperty ;
  rdfs:domain v5:FaceImage ;
  rdfs:range v5:TargetPerson ;
  rdfs:label "retrata pessoa"@pt , "depicts person"@en .

### Veículos / Detran

v5:ownsVehicle a owl:ObjectProperty ;
  rdfs:domain v5:Target ;
  rdfs:range v5:Vehicle ;
  owl:inverseOf v5:isOwnedBy ;
  rdfs:label "possui veículo"@pt , "owns vehicle"@en .

v5:isOwnedBy a owl:ObjectProperty ;
  rdfs:domain v5:Vehicle ;
  rdfs:range v5:Target ;
  rdfs:label "é de propriedade de"@pt , "is owned by"@en .

v5:hasLicensePlate a owl:ObjectProperty ;
  rdfs:domain v5:Vehicle ;
  rdfs:range v5:LicensePlate ;
  owl:inverseOf v5:isLicensePlateOf ;
  rdfs:label "tem placa"@pt , "has license plate"@en .

v5:isLicensePlateOf a owl:ObjectProperty ;
  rdfs:domain v5:LicensePlate ;
  rdfs:range v5:Vehicle ;
  rdfs:label "é placa de"@pt , "is license plate of"@en .

v5:hasVehicleRegistration a owl:ObjectProperty ;
  rdfs:domain v5:Vehicle ;
  rdfs:range v5:VehicleRegistration ;
  rdfs:label "tem registro veicular"@pt , "has vehicle registration"@en .

v5:hasMovementRecord a owl:ObjectProperty ;
  rdfs:domain v5:Vehicle ;
  rdfs:range v5:VehicleMovement ;
  rdfs:label "tem movimentação"@pt , "has movement record"@en .

v5:movementAtLocation a owl:ObjectProperty ;
  rdfs:domain v5:VehicleMovement ;
  rdfs:range v5:Location ;
  rdfs:label "movimentação em local"@pt , "movement at location"@en .

### Localização

v5:hasLocation a owl:ObjectProperty ;
  rdfs:domain prov:Entity ;
  rdfs:range v5:Location ;
  rdfs:label "ocorre em"@pt , "occurs at"@en .

########################
# DATATYPE PROPERTIES  #
########################

### Casos

v5:caseNumber a owl:DatatypeProperty ;
  rdfs:domain v5:Case ;
  rdfs:range xsd:string ;
  rdfs:label "número do caso"@pt , "case number"@en .

v5:caseName a owl:DatatypeProperty ;
  rdfs:domain v5:Case ;
  rdfs:range xsd:string ;
  rdfs:label "nome do caso"@pt , "case name"@en .

v5:caseStatus a owl:DatatypeProperty ;
  rdfs:domain v5:Case ;
  rdfs:range xsd:string ;
  rdfs:label "status do caso"@pt , "case status"@en .

### Alvos

v5:personFullName a owl:DatatypeProperty ;
  rdfs:domain v5:TargetPerson ;
  rdfs:range xsd:string ;
  rdfs:label "nome completo"@pt , "full name"@en , "nome"@pt .

v5:cpf a owl:DatatypeProperty ;
  rdfs:domain v5:TargetPerson ;
  rdfs:range xsd:string ;
  rdfs:label "CPF"@pt , "CPF"@en ;
  v5:normalizationType "numeric" ;
  v5:propertyPriority 1 ;
  v5:examplePattern "12345678900|98765432100|Localizar investigado com CPF <VALOR>" .

v5:cnpj a owl:DatatypeProperty ;
  rdfs:domain v5:TargetOrganization ;
  rdfs:range xsd:string ;
  rdfs:label "CNPJ"@pt , "CNPJ"@en .

v5:dateOfBirth a owl:DatatypeProperty ;
  rdfs:domain v5:TargetPerson ;
  rdfs:range xsd:date ;
  rdfs:label "data de nascimento"@pt , "date of birth"@en , "nascimento"@pt , "data nascimento"@pt , "aniversário"@pt .

### Telefones e dispositivos

v5:phoneNumberValue a owl:DatatypeProperty ;
  rdfs:domain v5:PhoneNumber ;
  rdfs:range xsd:string ;
  rdfs:label "número MSISDN"@pt , "MSISDN number"@en , "número"@pt , "número de telefone"@pt .

v5:deviceIMEI a owl:DatatypeProperty ;
  rdfs:domain v5:Device ;
  rdfs:range xsd:string ;
  rdfs:label "IMEI"@pt , "IMEI"@en , "imei"@pt .

v5:deviceModel a owl:DatatypeProperty ;
  rdfs:domain v5:Device ;
  rdfs:range xsd:string ;
  rdfs:label "modelo do dispositivo"@pt , "device model"@en , "modelo"@pt , "modelo aparelho"@pt .

### Mensagens / comunicação

v5:messageBody a owl:DatatypeProperty ;
  rdfs:domain v5:WhatsAppMessage ;
  rdfs:range xsd:string ;
  rdfs:label "corpo da mensagem"@pt , "message body"@en , "mensagem"@pt , "texto"@pt , "conteúdo"@pt .

v5:messageTimestamp a owl:DatatypeProperty ;
  rdfs:domain v5:WhatsAppMessage ;
  rdfs:range xsd:dateTime ;
  rdfs:label "data/hora da mensagem"@pt , "message timestamp"@en , "quando"@pt , "data mensagem"@pt , "hora mensagem"@pt .

### Arquivos / evidências

v5:filePath a owl:DatatypeProperty ;
  rdfs:domain v5:File ;
  rdfs:range xsd:string ;
  rdfs:label "caminho do arquivo"@pt , "file path"@en .

v5:fileHash a owl:DatatypeProperty ;
  rdfs:domain v5:File ;
  rdfs:range xsd:string ;
  rdfs:label "hash do arquivo"@pt , "file hash"@en .

v5:fileMimeType a owl:DatatypeProperty ;
  rdfs:domain v5:File ;
  rdfs:range xsd:string ;
  rdfs:label "MIME type"@pt , "MIME type"@en .

v5:ocrText a owl:DatatypeProperty ;
  rdfs:domain v5:OCRTextSegment ;
  rdfs:range xsd:string ;
  rdfs:label "texto OCR"@pt , "OCR text"@en .

### Biometria facial

v5:biometricTemplateType a owl:DatatypeProperty ;
  rdfs:domain v5:FaceBiometricTemplate ;
  rdfs:range xsd:string ;
  rdfs:label "tipo de template biométrico"@pt ,
             "biometric template type"@en .

v5:biometricTemplateData a owl:DatatypeProperty ;
  rdfs:domain v5:FaceBiometricTemplate ;
  rdfs:range xsd:string ;
  rdfs:label "dados do template biométrico (codificado)"@pt ,
             "biometric template data (encoded)"@en .

