  v5:normalizationType "alphanumeric" ;
  v5:propertyPriority 1 ;
  v5:examplePattern "ABC1234|ABC-1234|XYZ-9876|placa <VALOR>|dono do veículo de placa <VALOR>|dono do carro de placa <VALOR>" .

v5:renavam a owl:DatatypeProperty ;
  rdfs:domain v5:VehicleRegistration ;
  rdfs:range xsd:string ;
  rdfs:label "RENAVAM"@pt , "RENAVAM"@en , "renavam"@pt ;
  v5:normalizationType "numeric" ;
  v5:propertyPriority 1 ;
  v5:examplePattern "12345678901|98765432100" .

v5:chassisNumber a owl:DatatypeProperty ;
  rdfs:domain v5:Vehicle ;
  rdfs:range xsd:string ;
  rdfs:label "chassi"@pt , "chassis"@en , "número do chassi"@pt .

v5:vehicleBrand a owl:DatatypeProperty ;
  rdfs:domain v5:Vehicle ;
  rdfs:range xsd:string ;
  rdfs:label "marca do veículo"@pt , "marca"@pt , "vehicle brand"@en .

v5:vehicleModel a owl:DatatypeProperty ;
  rdfs:domain v5:Vehicle ;
  rdfs:range xsd:string ;
  rdfs:label "modelo do veículo"@pt , "modelo"@pt , "vehicle model"@en .

v5:vehicleYear a owl:DatatypeProperty ;
  rdfs:domain v5:Vehicle ;
  rdfs:range xsd:gYear ;
  rdfs:label "ano do veículo"@pt , "vehicle year"@en , "ano"@pt .

v5:movementTimestamp a owl:DatatypeProperty ;
  rdfs:domain v5:VehicleMovement ;
  rdfs:range xsd:dateTime ;
  rdfs:label "data/hora da movimentação"@pt ,
             "movement timestamp"@en ,
             "quando"@pt ,
             "data movimento"@pt .
#############################
# PROPRIEDADES DE ALTO NÍVEL
#############################

### Caso envolve alvo

v5:involvesTarget a owl:ObjectProperty ;
  rdfs:domain v5:Case ;
  rdfs:range v5:Target ;
  rdfs:label "envolve alvo"@pt , "involves target"@en .

### Caso envolve veículo

v5:involvesVehicle a owl:ObjectProperty ;
  rdfs:domain v5:Case ;
  rdfs:range v5:Vehicle ;
  rdfs:label "envolve veículo"@pt , "involves vehicle"@en .

### Caso envolve comunicação (mensagem, ligação etc.)

v5:involvesCommunication a owl:ObjectProperty ;
  rdfs:domain v5:Case ;
  rdfs:range v5:Communication ;
  rdfs:label "envolve comunicação"@pt , "involves communication"@en .

### Alvo se comunicou com outro alvo

v5:communicatedWith a owl:ObjectProperty , owl:SymmetricProperty ;
  rdfs:domain v5:Target ;
  rdfs:range v5:Target ;
  rdfs:label "se comunicou com"@pt , "communicated with"@en .

#############################
# RESTRIÇÕES DE TIPO (EXEMPLOS)
#############################

# Toda mensagem de WhatsApp:
# - é enviada por pelo menos um alvo
# - pertence a pelo menos um chat

v5:WhatsAppMessage rdfs:subClassOf
  [ a owl:Restriction ;
    owl:onProperty v5:sentBy ;
    owl:someValuesFrom v5:Target
  ] ,
  [ a owl:Restriction ;
    owl:onProperty v5:belongsToChat ;
    owl:someValuesFrom v5:WhatsAppChat
  ] .

# Defina a propriedade belongsToChat se ainda não existir:
v5:belongsToChat a owl:ObjectProperty ;
  rdfs:domain v5:WhatsAppMessage ;
  rdfs:range v5:WhatsAppChat ;
  rdfs:label "pertence ao chat"@pt , "belongs to chat"@en .


#############################
# CHAINS: CASO -> MENSAGEM -> ALVO
#############################

# 1) Caso -> Mensagens ligadas a ele
# Cria propriedade intermediária: hasMessageEvidence

v5:hasMessageEvidence a owl:ObjectProperty ;
  rdfs:label "tem mensagem como evidência"@pt ,
             "has message evidence"@en ;
  rdfs:domain v5:Case ;
  rdfs:range  v5:WhatsAppMessage .

# hasEvidence o belongsToExtraction o hasChat o hasMessage
#   ⊑ hasMessageEvidence
#
# (lido: se um Caso tem Evidência, que pertence a uma Extração,
#        que tem um Chat com uma Mensagem, então o Caso tem essa Mensagem como evidência)

v5:hasMessageEvidence owl:propertyChainAxiom (
  v5:hasEvidence
  v5:belongsToExtraction
  v5:hasChat
  v5:hasMessage
) .

# 2) Caso com mensagem enviada por um alvo -> envolve esse alvo
# hasMessageEvidence o sentBy ⊑ involvesTarget

v5:involvesTarget owl:propertyChainAxiom (
  v5:hasMessageEvidence
  v5:sentBy
) .


#############################
# CHAINS: CASO -> IMAGEM FACIAL -> PESSOA
#############################

# hasEvidence o hasFaceImage o depictsPerson ⊑ involvesTarget

v5:involvesTarget owl:propertyChainAxiom (
  v5:hasEvidence
  v5:hasFaceImage
  v5:depictsPerson
) .

#############################
# CHAINS: CASO -> VEÍCULO -> ALVO
#############################

# Propriedade auxiliar: hasVehicleEvidence
v5:hasVehicleEvidence a owl:ObjectProperty ;
  rdfs:label "tem veículo como evidência"@pt ,
             "has vehicle as evidence"@en ;
  rdfs:domain v5:Case ;
  rdfs:range  v5:Vehicle .

# Supondo que certas evidências sejam diretamente do tipo Vehicle (ou relacionadas):
# hasEvidence o ownsVehicle^-1 ⊑ hasVehicleEvidence
# (mais tarde, se precisar, refinamos com VehicleRegistration, Movement etc.)

# Primeiro, uma propriedade inversa de ownsVehicle, se ainda não foi feita:
v5:ownsVehicle owl:inverseOf v5:isOwnedBy .

# Agora, a chain: Case --hasEvidence--> Vehicle --isOwnedBy--> Target
# vira: Case --involvesVehicle--> Vehicle e Case --involvesTarget--> Target

v5:involvesVehicle owl:propertyChainAxiom (
  v5:hasEvidence
) .

v5:involvesTarget owl:propertyChainAxiom (
  v5:hasEvidence
  v5:isOwnedBy
) .


#############################
# ESPECIALIZAÇÕES DE HAS TARGET
#############################

v5:hasPrimaryTarget a owl:ObjectProperty ;
  rdfs:subPropertyOf v5:hasTarget ;
  rdfs:domain v5:Case ;
  rdfs:range v5:Target ;
  rdfs:label "tem alvo principal"@pt , "has primary target"@en .

v5:hasSuspectTarget a owl:ObjectProperty ;
  rdfs:subPropertyOf v5:hasTarget ;
  rdfs:domain v5:Case ;
  rdfs:range v5:TargetPerson ;
  rdfs:label "tem alvo suspeito"@pt , "has suspect target"@en .

v5:hasVictimTarget a owl:ObjectProperty ;
  rdfs:subPropertyOf v5:hasTarget ;
  rdfs:domain v5:Case ;
  rdfs:range v5:TargetPerson ;
  rdfs:label "tem alvo vítima"@pt , "has victim target"@en .

v5:hasWitnessTarget a owl:ObjectProperty ;
  rdfs:subPropertyOf v5:hasTarget ;
  rdfs:domain v5:Case ;
  rdfs:range v5:TargetPerson ;
  rdfs:label "tem alvo testemunha"@pt , "has witness target"@en .

# Nome alternativo para isTargetOf (semântica igual, ajuda a leitura)
v5:participatesInCase a owl:ObjectProperty ;
  owl:equivalentProperty v5:isTargetOf ;
  rdfs:label "participa do caso"@pt , "participates in case"@en ;
  rdfs:domain v5:Target ;
  rdfs:range  v5:Case .

#############################
# RESTRIÇÕES EM CLASSES
#############################

# Todo Caso tem pelo menos um alvo
v5:Case rdfs:subClassOf
  [ a owl:Restriction ;
    owl:onProperty v5:hasTarget ;
    owl:someValuesFrom v5:Target
  ] .

# Todo Chat de WhatsApp tem pelo menos um participante e pelo menos uma mensagem
v5:WhatsAppChat rdfs:subClassOf
  [ a owl:Restriction ;
    owl:onProperty v5:hasParticipant ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger
  ] ,
  [ a owl:Restriction ;
    owl:onProperty v5:hasMessage ;
    owl:someValuesFrom v5:WhatsAppMessage
  ] .

# Todo Veículo tem alguma placa e algum registro veicular
v5:Vehicle rdfs:subClassOf
  [ a owl:Restriction ;
    owl:onProperty v5:hasLicensePlate ;
    owl:someValuesFrom v5:LicensePlate
  ] ,
  [ a owl:Restriction ;
    owl:onProperty v5:hasVehicleRegistration ;
    owl:someValuesFrom v5:VehicleRegistration
  ] .

# Toda Movimentação de Veículo está associada a um Veículo e a uma Localização
v5:VehicleMovement rdfs:subClassOf
  [ a owl:Restriction ;
    owl:onProperty v5:isMovementOf ;
    owl:someValuesFrom v5:Vehicle
  ] ,
  [ a owl:Restriction ;
    owl:onProperty v5:movementAtLocation ;
    owl:someValuesFrom v5:Location
  ] .

#############################
# NOVAS PROPRIEDADES AUXILIARES
#############################

# Inverso explícito de hasParticipant para chains
v5:participatesInChat a owl:ObjectProperty ;
  rdfs:domain v5:Target ;
  rdfs:range v5:WhatsAppChat ;
  owl:inverseOf v5:hasParticipant ;
  rdfs:label "participa do chat"@pt , "participates in chat"@en .

# Evidência que fala sobre um alvo
v5:evidenceAboutTarget a owl:ObjectProperty ;
  rdfs:domain v5:Entity ;
  rdfs:range v5:Target ;
  rdfs:label "é evidência sobre alvo"@pt , "is evidence about target"@en .

# Caso envolve dispositivo
v5:involvesDevice a owl:ObjectProperty ;
  rdfs:domain v5:Case ;
  rdfs:range v5:Device ;
  rdfs:label "envolve dispositivo"@pt , "involves device"@en .

# Inversos mais explícitos para Detran
v5:isRegistrationOf a owl:ObjectProperty ;
  rdfs:domain v5:VehicleRegistration ;
  rdfs:range v5:Vehicle ;
  owl:inverseOf v5:hasVehicleRegistration ;
  rdfs:label "é registro de veículo"@pt , "is registration of vehicle"@en .

v5:isMovementOf a owl:ObjectProperty ;
  rdfs:domain v5:VehicleMovement ;
  rdfs:range v5:Vehicle ;
  owl:inverseOf v5:hasMovementRecord ;
  rdfs:label "é movimentação de"@pt , "is movement of"@en .

#############################
# PROPERTY CHAINS – COMUNICAÇÃO
#############################

# Caso -> Mensagem (já existia hasMessageEvidence; usamos ela)

# Caso que tem evidência ligada a extração, chat e mensagem -> envolve comunicação
v5:involvesCommunication owl:propertyChainAxiom (
  v5:hasEvidence
  v5:belongsToExtraction
  v5:hasChat
  v5:hasMessage
) .

# Target que participa de um chat onde há mensagem enviada por outro Target
# -> se comunicou com
v5:communicatedWith owl:propertyChainAxiom (
  v5:participatesInChat
  v5:hasMessage
  v5:sentBy
) .

#############################
# PROPERTY CHAINS – DISPOSITIVOS
#############################

# Caso -> evidência -> extração -> dispositivo -> Caso envolve dispositivo
v5:involvesDevice owl:propertyChainAxiom (
  v5:hasEvidence
  v5:belongsToExtraction
  v5:wasExtractedFromDevice
) .

#############################
# PROPERTY CHAINS – VEÍCULOS
#############################

# Caso tem evidência que é registro de veículo -> caso envolve veículo
v5:involvesVehicle owl:propertyChainAxiom (
  v5:hasEvidence
  v5:isRegistrationOf
) .

# Caso tem evidência que é movimentação de veículo -> caso envolve veículo
v5:involvesVehicle owl:propertyChainAxiom (
  v5:hasEvidence
  v5:isMovementOf
) .

#############################
# PROPERTY CHAINS – EVIDÊNCIA SOBRE ALVOS
#############################

# Evidência que tem imagem facial que retrata pessoa -> evidência sobre alvo
v5:evidenceAboutTarget owl:propertyChainAxiom (
  v5:hasFaceImage
  v5:depictsPerson
) .

# Evidência que é mensagem (via hasMessageEvidence) enviada por alvo -> evidência sobre alvo
v5:evidenceAboutTarget owl:propertyChainAxiom (
  v5:hasMessageEvidence
  v5:sentBy
) .

# Evidência que é mensagem recebida por alvo -> evidência sobre alvo
v5:evidenceAboutTarget owl:propertyChainAxiom (
  v5:hasMessageEvidence
  v5:sentTo
) .

#############################
# PROPERTY CHAINS – CASO <-> ALVO VIA EVIDÊNCIA
#############################

# Caso com evidência sobre alvo -> envolve alvo
v5:involvesTarget owl:propertyChainAxiom (
  v5:hasEvidence
  v5:evidenceAboutTarget
) .

