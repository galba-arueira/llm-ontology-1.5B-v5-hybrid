#############################
# METADADOS PARA INTENT GENERATION (METADATA-DRIVEN)
#############################
# Annotation properties e metadados que permitem geração genérica de intents
# sem hardcoding de domínio específico no código Python.
#############################

# do make_v5_intents.py, tornando-o genérico para qualquer ontologia.
# ============================================================================

# 1. Tipo de Normalização
v5:normalizationType a owl:AnnotationProperty ;
    rdfs:label "Tipo de Normalização"@pt , "Normalization Type"@en ;
    rdfs:comment "Define como a propriedade deve ser normalizada em queries Cypher: 'numeric' (apenas dígitos), 'alphanumeric' (remove -, espaços), 'text' (sem normalização exata), 'text_contains' (busca parcial com CONTAINS)"@pt ;
    rdfs:range xsd:string .

# 2. Prioridade da Propriedade
v5:propertyPriority a owl:AnnotationProperty ;
    rdfs:label "Prioridade da Propriedade"@pt , "Property Priority"@en ;
    rdfs:comment "Indica a importância da propriedade para intents compostos: 1 (alta - primary identifier), 2 (média), 3 (baixa)"@pt ;
    rdfs:range xsd:integer .

# 3. Padrões de Exemplo
v5:examplePattern a owl:AnnotationProperty ;
    rdfs:label "Padrão de Exemplo"@pt , "Example Pattern"@en ;
    rdfs:comment "Exemplos de queries em linguagem natural para esta propriedade, separados por | (pipe). Use <VALOR> como placeholder."@pt ;
    rdfs:range xsd:string .

# 4. Padrões de Exemplo Compostos
v5:compositeExamplePattern a owl:AnnotationProperty ;
    rdfs:label "Padrão de Exemplo Composto"@pt , "Composite Example Pattern"@en ;
    rdfs:comment "Exemplos para queries compostas multi-hop envolvendo esta classe como nó inicial, separados por | (pipe)"@pt ;
    rdfs:range xsd:string .

# 5. Propriedades Importantes
v5:hasImportantProperty a owl:AnnotationProperty ;
    rdfs:label "Tem Propriedade Importante"@pt , "Has Important Property"@en ;
    rdfs:comment "Lista de propriedades importantes para esta classe (para incluir em intents compostos), separadas por vírgula"@pt ;
    rdfs:range xsd:string .

# ============================================================================
# APLICAÇÃO DOS METADADOS ÀS CLASSES
# ============================================================================

# TARGETPERSON: Propriedades importantes = cpf, personFullName
v5:TargetPerson 
    v5:hasImportantProperty "cpf,personFullName" ;
    v5:compositeExamplePattern "Dono do veículo <VALOR>|De quem é o veículo <VALOR>?|Buscar caso do investigado com CPF <VALOR>|Caso do CPF <VALOR>" .


# CASE: Examples compostos para queries multi-hop
v5:Case 
    v5:compositeExamplePattern "Caso do veículo placa <VALOR>|Quais casos estão associados ao veículo de placa <VALOR>?|Caso do CPF <VALOR>|Procedimento do CPF <VALOR>" .

# VEHICLE: Examples para entidades relacionadas
v5:Vehicle 
    v5:compositeExamplePattern "Veículo do CPF <VALOR>|Carro da placa <VALOR>" .

# ============================================================================
# APLICAÇÃO DOS METADADOS ÀS DATATYPE PROPERTIES
# ============================================================================

### PROPRIEDADES NUMÉRICAS (normalizationType = "numeric")

# CPF: High priority, numeric normalization
v5:cpf 
    v5:normalizationType "numeric" ;
    v5:propertyPriority 1 ;
    v5:examplePattern "CPF <VALOR>|Localizar investigado com CPF <VALOR>|Investigado CPF <VALOR>|Buscar pessoa com CPF <VALOR>" .

# phoneNumberValue: High priority, numeric
v5:phoneNumberValue 
    v5:normalizationType "numeric" ;
    v5:propertyPriority 1 ;
    v5:examplePattern "Telefone <VALOR>|Celular <VALOR>|Número <VALOR>|Localizar celular <VALOR>" .

# renavam: High priority, numeric
v5:renavam 
    v5:normalizationType "numeric" ;
    v5:propertyPriority 1 ;
    v5:examplePattern "RENAVAM <VALOR>|Registro <VALOR>|Buscar RENAVAM <VALOR>" .

# deviceIMEI: High priority, numeric
v5:deviceIMEI 
    v5:normalizationType "numeric" ;
    v5:propertyPriority 1 ;
    v5:examplePattern "IMEI <VALOR>|Equipamento <VALOR>|Dispositivo IMEI <VALOR>|Localizar IMEI <VALOR>" .

# caseNumber: High priority, numeric
v5:caseNumber 
    v5:normalizationType "numeric" ;
    v5:propertyPriority 1 ;
    v5:examplePattern "Caso <VALOR>|Procedimento <VALOR>|Buscar procedimento <VALOR>|Localizar procedimento <VALOR>|procedimento <VALOR>|Inquérito <VALOR>|Número do caso <VALOR>|Buscar caso <VALOR>" .

### PROPRIEDADES ALFANUMÉRICAS (normalizationType = "alphanumeric")

# licensePlateNumber: High priority, alphanumeric (remove hyphens/spaces)
v5:licensePlateNumber 
    v5:normalizationType "alphanumeric" ;
    v5:propertyPriority 1 ;
    v5:examplePattern "Placa <VALOR>|Placa ABC-1D23|Placa ABC 1D23|ABC-1D23|Placa do carro <VALOR>|Placa veicular <VALOR>|Buscar placa <VALOR>|Veículo placa <VALOR>" .

### PROPRIEDADES DE TEXTO (normalizationType = "text")

# personFullName: High priority, text (no normalization)
v5:personFullName 
    v5:normalizationType "text" ;
    v5:propertyPriority 1 ;
    v5:examplePattern "Nome <VALOR>|Pessoa <VALOR>|Buscar pessoa <VALOR>|Nome completo <VALOR>" .

# vehicleBrand: Medium priority, text
v5:vehicleBrand 
    v5:normalizationType "text" ;
    v5:propertyPriority 2 ;
    v5:examplePattern "Moto <VALOR>|Carro <VALOR>|Veículo <VALOR>|Marca <VALOR>|Buscar veículo marca <VALOR>|Buscar moto <VALOR>" .

# vehicleModel: Medium priority, text
v5:vehicleModel 
    v5:normalizationType "text" ;
    v5:propertyPriority 2 ;
    v5:examplePattern "Modelo <VALOR>|Veículo modelo <VALOR>|Carro modelo <VALOR>" .

# vehicleYear: Medium priority, text (year is number but no normalization needed)
v5:vehicleYear 
    v5:normalizationType "text" ;
    v5:propertyPriority 2 ;
    v5:examplePattern "Ano <VALOR>|Veículo ano <VALOR>|Carro ano <VALOR>|Ano do veículo <VALOR>" .

# caseStatus: Low priority, text
v5:caseStatus 
    v5:normalizationType "text" ;
    v5:propertyPriority 3 ;
    v5:examplePattern "Status <VALOR>|Caso com status <VALOR>|Status do caso <VALOR>" .

# deviceModel: Low priority, text
v5:deviceModel 
    v5:normalizationType "text" ;
    v5:propertyPriority 3 ;
    v5:examplePattern "Modelo do dispositivo <VALOR>|Aparelho modelo <VALOR>" .

# messageBody: Low priority, text
v5:messageBody 
    v5:normalizationType "text" ;
    v5:propertyPriority 3 ;
    v5:examplePattern "Mensagem <VALOR>|Mensagem contendo <VALOR>|Texto da mensagem <VALOR>" .


